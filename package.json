{
  "name": "uber-eats-frontend",
  "version": "1.0.0",
  "description": "<div align=\"center\">\r   <h1>React Food Delivery</h1>",
  "main": ".eslintrc.js",
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "cypress": "^13.6.3",
    "graphql": "^16.8.1",
    "mapbox-gl": "^3.1.2",
    "react": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.3",
    "react-mapbox-gl": "^5.1.1",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1",
    "victory": "^36.8.2",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.6",
    "import-sort-parser-typescript": "^6.0.0",
    "lint-staged": "^15.2.0",
    "mock-apollo-client": "^1.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "apollo:codegen": "rm -rf src/types && apollo client:codegen src/types --target=typescript --outputFlat",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json|css|scss|gql|graphql)\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "react-scripts start",
    "test": "react-scripts test --verbose",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "typecheck": "tsc -b"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/app.tsx",
      "./src/components/**/*.{js,jsx,ts,tsx}",
      "./src/pages/**/*.{js,jsx,ts,tsx}",
      "./src/routers/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "author": "Dattaraj Dhadas",
  "license": "ISC"
}